@model IEnumerable<Unidad2Actividad5.Models.Clase>
@{
    Layout = "_Layout";
}
<div class="slides">
    <img src="images/carousel1.jpg">
</div>

<h1>Clasificación de los vertebrados</h1>
<div class="clases">
    @foreach (var clas in Model)
    {
        <div>
            <a href="~/@clas.Nombre.Replace(" ", "-")">
                <img src="~/clases/@(clas.Id).jpg">
                <h1>@clas.Nombre</h1>
                <p>@clas.Descripcion</p>
            </a>
        </div>
    }
</div>

